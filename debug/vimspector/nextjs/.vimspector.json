{
  "configurations": {
    "run": {
      "adapter": "js-debug",
      "breakpoints": {
        "exception": {
          "all": "",
          "caught": "",
          "uncaught": "",
          "userUnhandled": "",
          "raised": ""
        }
      },
      "configuration": {
        "name": "Attach to Next.js server",
        "request": "attach",
        "type": "pwa-node",
        "cwd": "${workspaceRoot}",
        "processId": "${processId}"
      }
    }
  }
}
