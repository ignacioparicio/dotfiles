{
  "configurations": {
    "run": {
      "adapter": "js-debug",
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      },
      "configuration": {
        "name": "Attach to Next.js server",
        "request": "attach",
        "type": "pwa-node",
        "cwd": "${workspaceRoot}",
        "processId": "${processId}"
      }
    }
  }
}
